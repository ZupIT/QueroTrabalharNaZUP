<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.mobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity android:name="br.com.mobile.model.Principal">
            
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        <activity android:name="br.projeto.telas.Principais_Conquistas"></activity>
        <activity android:name="br.projeto.telas.Informacao"></activity>
        <activity android:name="br.projeto.telas.Motivacoes"></activity>
        <activity android:name="br.projeto.telas.Objetivos"></activity>
    </application>

</manifest>
